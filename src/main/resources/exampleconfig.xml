<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd"
       xmlns:p="http://www.springframework.org/schema/p">

    <bean class="org.ioc.example.setter.injection.SetterInjectionExample1" name="setterex1" id="setterex1" />
    <bean class="org.ioc.example.constructor.injection.ConstructorInjectionExample1" name="constructex1" id="constructex1" />
    <bean class="org.ioc.example.auto.wire.AutoWireRunner" name="autowireex1" id="autowireex1"/>
    <bean class="org.ioc.example.annotation.auto.wire.AnnotationAutoWire" name="annoex1" id="annoex1"/>
    <bean class="org.ioc.example.stand.alone.collections.StandAloneCollection" name="standex1" id="standex1"/>
    <bean class="org.ioc.example.stereotype.annotation.StereoTypeAnnotationExample1" name="stereo1" id="stereo1"/>
    <bean class="org.ioc.example.java.configuration.JavaConfigurationExample" name="jcex1" id="jcex1"/>
    <bean class="org.example.App" name="app1" id="app1"/>

    <context:annotation-config/>
</beans>